super + Return
	urxvt

super + d
	rofi -modi run,drun,ssh -show drun -columns 2 -hide-scrollbar -show-icons -drun-icon-theme "la-capitaine-icon-theme"

super + Escape
  runsxhkd.sh

super + shift + a
    $HOME/.config/polybar/scripts/audiodevice.sh

super + shift + w
    $HOME/.config/polybar/scripts/wifi.sh

super + shift + t
    XDG_CURRENT_DESKTOP=GNOME gnome-tweaks

super + shift + s
    XDG_CURRENT_DESKTOP=GNOME gnome-control-center


XF86AudioRaiseVolume
    amixer -D pulse set Master 5%+
XF86AudioLowerVolume 
    amixer -D pulse set Master 5%-
XF86AudioMute 
    amixer -D pulse set Master 1+ toggle
XF86AudioNext 
    mpc --host /home/risus/.local/run/mpd.socket next
XF86AudioPrev 
    mpc --host /home/risus/.local/run/mpd.socket prev
XF86AudioPlay 
    mpc --host /home/risus/.local/run/mpd.socket toggle
XF86AudioStop 
    mpc --host /home/risus/.local/run/mpd.socket stop

Print 
    scr.sh
super + Pause 
    $XDG_CONFIG_HOME/polybar/scripts/powermenu
