super + Return
	urxvt

super + d
	rofi -modi run,drun,ssh -show drun -columns 2 -hide-scrollbar -show-icons

super + Escape
	pkill -USR1 -x sxhkd


super + shift + a
    $HOME/.config/polybar/scripts/audiodevice.sh

super + shift + w
    $HOME/.config/polybar/scripts/wifi.sh

super + shift + t
    XDG_CURRENT_DESKTOP=GNOME gnome-tweaks

super + shift + s
    XDG_CURRENT_DESKTOP=GNOME gnome-control-center


XF86AudioRaiseVolume
    amixer -D pulse set Master 5%+
XF86AudioLowerVolume 
    amixer -D pulse set Master 5%-
XF86AudioMute 
    amixer -D pulse set Master 1+ toggle
XF86AudioNext 
    mpc --host $MPDS next
XF86AudioPrev 
    mpc --host $MPDS prev
XF86AudioPlay 
    mpc --host $MPDS toggle
XF86AudioStop 
    mpc --host $MPDS stop

Print 
    $HOME/.local/sh/scr.sh
super + Pause 
    $XDG_CONFIG_HOME/polybar/scripts/powermenu

