alt + d
    ${HOME}/.config/rofi/bins/appsmenu

alt + a
    ${HOME}/.config/rofi/bins/mpdmenu

super + Pause
    ${HOME}/.config/rofi/bins/powermenu

super + shift + t
    XDG_CURRENT_DESKTOP=GNOME gnome-tweaks

super + shift + s
    XDG_CURRENT_DESKTOP=GNOME gnome-control-center

super + Return
    urxvt

super + Escape
    pkill -USR1 -x sxhkd

